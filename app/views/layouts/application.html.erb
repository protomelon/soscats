<!DOCTYPE html>
<html>
  <head>
    <title>Soscats</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <ul>
      <li><%= link_to 'Home', :root %></li>
      <li><%= link_to 'About Us', :about %></li>
      <li><%= link_to 'Cats', :cats %></li>
      <li><%= link_to 'Trees', :trees %></li>
      <li><%= link_to 'Cat Breeds', :breeds %></li>
    </ul>

    <%= yield %>
  </body>
</html>
